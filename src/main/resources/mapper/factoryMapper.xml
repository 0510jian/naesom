<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="som.som.factory.factoryMapper">
    <select id="selectFactory" resultType="som.som.DTO.factoryDTO">
        select *
        from factory
    </select>

    <select id="selectFactorySize" resultType="som.som.DTO.factorySizeDTO">
        select *
        from factory_size
    </select>

    <select id="selectSize" resultType="som.som.DTO.sizeDTO">
        select *
        from size
    </select>
</mapper>